<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Searching_Pane_Controller">
    <children>
        <BorderPane fx:id="BorderPaneId" layoutX="114.0" layoutY="55.0" prefHeight="420.0" prefWidth="577.0">
            <center>
                <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                    <children>
                        <TextField fx:id="txtWord" alignment="CENTER" layoutX="22.0" layoutY="14.0" prefHeight="30.0" prefWidth="177.0" text="Enter English Word">
                            <font>
                                <Font size="14.0" />
                            </font>
                        </TextField>
                  <Text fx:id="txtTarget" layoutX="234.0" layoutY="34.0" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="297.7294921875" />
                        <Text fx:id="txtDefinition" layoutX="234.0" layoutY="56.0" strokeType="OUTSIDE" strokeWidth="0.0" />
                  <Button fx:id="btnSpeck" layoutX="521.0" layoutY="17.0" mnemonicParsing="false" onAction="#Speck" text="speck" />
                  <Button fx:id="btnAddNotes" layoutX="518.0" layoutY="59.0" mnemonicParsing="false" text="add notes" onAction="#addNotes"/>
                    </children>
                </AnchorPane>
            </center>
        </BorderPane>

        <VBox layoutX="14.0" layoutY="69.0" prefHeight="210.0" prefWidth="100.0">
            <children>
                <Button mnemonicParsing="false" onAction="#LoadSearchingButton" prefHeight="30.0" prefWidth="100.0" text="Search" textAlignment="CENTER" />
                <Button mnemonicParsing="false" onAction="#LoadEditingButton" prefHeight="30.0" prefWidth="100.0" text="Edit" textAlignment="CENTER" />
                <Button mnemonicParsing="false" onAction="#LoadHistoryButton" prefHeight="30.0" prefWidth="100.0" text="History" />
                <Button mnemonicParsing="false" onAction="#LoadNoteButton" prefHeight="30.0" prefWidth="100.0" text="Note" />
                <Button mnemonicParsing="false" onAction="#LoadAboutUsButton" prefHeight="30.0" prefWidth="100.0" text="About us" />
            </children>
        </VBox>
    </children>
</AnchorPane>
